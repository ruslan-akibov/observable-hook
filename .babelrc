{
  "presets": [
     ["@babel/preset-env", {
       "modules": false,
       "useBuiltIns": "entry",
       "loose": true,
       "corejs": 3
     }],
     "@babel/preset-react"
  ],
  "plugins": [
     ["@babel/plugin-transform-runtime", { "corejs": 3 }],
     "@babel/plugin-proposal-class-properties",
     ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ]
}
